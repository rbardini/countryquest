{
  "name": "countryquest",
  "version": "0.1.0",
  "private": true,
  "description": "An interactive scratch off map. Keep track of which places you have been, how much of the world you have conquered, and where to go next.",
  "keywords": [
    "scratch",
    "map",
    "world",
    "countries",
    "travel",
    "tourism",
    "visits",
    "achievements"
  ],
  "homepage": "https://country.quest/",
  "bugs": {
    "url": "https://github.com/rbardini/countryquest/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/countryquest.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Rafael Bardini",
    "email": "rafael@rbardini.com",
    "url": "https://rbardini.com/"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "format": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "lint": "next lint",
    "prepare": "husky install",
    "restcountries:fetch": "curl https://restcountries.com/v3.1/all | jq 'INDEX(.cca2)' > data/preval/json/restcountries.json",
    "start": "next start",
    "type:check": "tsc"
  },
  "dependencies": {
    "@amcharts/amcharts5": "5.1.10",
    "@amcharts/amcharts5-geodata": "5.0.3",
    "@chakra-ui/icons": "1.1.7",
    "@chakra-ui/react": "1.8.7",
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@fontsource/dosis": "4.5.7",
    "@next/bundle-analyzer": "12.1.4",
    "@supabase/supabase-js": "1.33.2",
    "boring-avatars": "1.6.3",
    "framer-motion": "6.2.8",
    "is-eu-member": "1.0.6",
    "jotai": "1.6.1",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "babel-plugin-preval": "5.1.0",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-packagejson": "2.2.17",
    "typescript": "4.6.3"
  }
}
