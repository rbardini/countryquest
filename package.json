{
  "name": "countryquest",
  "version": "0.1.0",
  "private": true,
  "description": "An interactive scratch off map. Keep track of which places you have been, how much of the world you have conquered, and where to go next.",
  "keywords": [
    "scratch",
    "map",
    "world",
    "countries",
    "travel",
    "tourism",
    "visits",
    "achievements"
  ],
  "homepage": "https://country.quest/",
  "bugs": {
    "url": "https://github.com/rbardini/countryquest/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/countryquest.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Rafael Bardini",
    "email": "rafael@rbardini.com",
    "url": "https://rbardini.com/"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "format": "prettier --ignore-path .gitignore --write .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "lint": "next lint",
    "prepare": "husky install",
    "start": "next start",
    "supabase:init": "supabase init",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "type:check": "tsc"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.10.24",
    "@amcharts/amcharts4-geodata": "4.1.22",
    "@chakra-ui/icons": "1.1.5",
    "@chakra-ui/react": "1.8.3",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@fontsource/dosis": "4.5.4",
    "@mapbox/geojson-area": "0.2.2",
    "@next/bundle-analyzer": "12.0.10",
    "@supabase/supabase-js": "1.30.2",
    "boring-avatars": "1.6.1",
    "country-code-emoji": "2.3.0",
    "framer-motion": "6.2.6",
    "jotai": "1.6.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "babel-plugin-preval": "5.1.0",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-packagejson": "2.2.15",
    "supabase": "0.5.0",
    "typescript": "4.5.5"
  }
}
