{
  "name": "countryquest",
  "version": "0.1.0",
  "private": true,
  "description": "An interactive scratch off map. Keep track of which places you have been, how much of the world you have conquered, and where to go next.",
  "keywords": [
    "scratch",
    "map",
    "world",
    "countries",
    "travel",
    "tourism",
    "visits",
    "achievements"
  ],
  "homepage": "https://country.quest/",
  "bugs": {
    "url": "https://github.com/rbardini/countryquest/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/countryquest.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Rafael Bardini",
    "email": "rafael@rbardini.com",
    "url": "https://rbardini.com/"
  },
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "prepare": "husky install",
    "restcountries:fetch": "curl https://restcountries.com/v3.1/all | jq 'INDEX(.cca2)' > data/preval/json/restcountries.json",
    "start": "next start",
    "type:check": "tsc"
  },
  "dependencies": {
    "@amcharts/amcharts5": "5.4.1",
    "@amcharts/amcharts5-geodata": "5.1.1",
    "@chakra-ui/icons": "2.1.0",
    "@chakra-ui/react": "2.8.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource-variable/dosis": "5.0.5",
    "@next/bundle-analyzer": "13.4.12",
    "@supabase/supabase-js": "2.29.0",
    "boring-avatars": "1.10.1",
    "framer-motion": "10.13.1",
    "is-eu-member": "1.0.6",
    "jotai": "2.2.2",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.4.4",
    "@types/react": "18.2.15",
    "babel-plugin-preval": "5.1.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.3",
    "prettier-plugin-packagejson": "2.4.5",
    "typescript": "5.1.6"
  }
}
